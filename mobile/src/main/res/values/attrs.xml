<?xml version="1.0" encoding="utf-8"?>
<resources>

  <declare-styleable name="WutsonTextView">
    <attr name="textFont" format="enum">
      <enum name="none" value="-1" />
      <enum name="robotoMedium" value="0" />
      <enum name="robotoRegular" value="1" />
    </attr>
  </declare-styleable>

  <declare-styleable name="AppBarWidget">
    <attr name="mergeLayoutId" format="reference" />
    <attr name="appBarBackgroundColor" format="color" />
  </declare-styleable>

  <declare-styleable name="AspectRatioImageView">
    <attr name="aspectRatio" />
  </declare-styleable>

  <declare-styleable name="AspectRatioFrameLayout">
    <attr name="aspectRatio" />
  </declare-styleable>

  <attr name="aspectRatio" format="string" />

</resources>
